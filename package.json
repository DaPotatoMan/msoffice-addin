{
  "name": "msoffice-addin",
  "type": "module",
  "version": "0.0.8",
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a",
  "description": "Microsoft office addin module for Nuxt abd vite",
  "author": "Fahadul Islam @dapotatoman",
  "license": "MIT",
  "repository": "dapotatoman/msoffice-addin",
  "keywords": [
    "office-addin",
    "nuxt",
    "vite-plugin"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.1",
    "fs-extra": "^11.2.0",
    "vite": "^7.2.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.11.6",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint": "0.3.0-beta.7",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^4.2.1",
    "@nuxt/test-utils": "^3.12.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.2",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "nuxt": "^4.2.1",
    "office-addin-manifest-converter": "^0.2.4",
    "office-addin-validator": "^1.0.5",
    "vitest": "^4.0.15"
  }
}
